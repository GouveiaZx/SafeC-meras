{
  "name": "newcam-backend",
  "version": "1.0.0",
  "description": "Backend do sistema de vigilância por câmeras IP NewCAM - Updated",
  "main": "src/server.js",
  "type": "module",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "build": "echo 'No build step required for Node.js'",
    "init": "node src/scripts/initializeServices.js",
    "start:prod": "npm run init && npm start",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.js",
    "lint:fix": "eslint src/**/*.js --fix",
    "format": "prettier --write src/**/*.js",
    "migrate": "node src/database/migrate.js",
    "seed": "node src/database/seed.js",
    "db:reset": "npm run migrate && npm run seed",
    "services:check": "node src/scripts/initializeServices.js --check-only",
    "cleanup": "node src/scripts/cleanup.js"
  },
  "keywords": [
    "surveillance",
    "camera",
    "ip-camera",
    "rtsp",
    "rtmp",
    "streaming",
    "security",
    "monitoring",
    "newcam"
  ],
  "author": "NewCAM Team",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^6.13.0",
    "@sendgrid/mail": "^8.1.0",
    "@supabase/supabase-js": "^2.38.4",
    "archiver": "^7.0.1",
    "aws-sdk": "^2.1692.0",
    "axios": "^1.11.0",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^2.4.3",
    "bull": "^4.12.2",
    "chokidar": "^4.0.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "cron": "^3.1.6",
    "dotenv": "^16.3.1",
    "express": "^4.21.2",
    "express-rate-limit": "^7.1.5",
    "express-slow-down": "^2.0.1",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "ioredis": "^5.3.2",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.14.2",
    "node-cron": "^4.2.1",
    "node-rtsp-stream": "^0.0.9",
    "nodemailer": "^6.9.7",
    "redis": "^4.6.10",
    "socket.io": "^4.7.4",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@types/jest": "^29.5.8",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "prettier": "^3.1.0",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/newcam/backend.git"
  },
  "bugs": {
    "url": "https://github.com/newcam/backend/issues"
  },
  "homepage": "https://github.com/newcam/backend#readme"
}
