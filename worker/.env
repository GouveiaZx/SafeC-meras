# Configurações do Worker NewCAM - Desenvolvimento Local

# URL do Backend (desenvolvimento local)
BACKEND_URL=http://localhost:3002

# Token de autenticação do worker
WORKER_TOKEN=newcam-worker-token-2025-secure

# Configurações de log
LOG_LEVEL=debug
VERBOSE_LOGGING=true

# Configurações de monitoramento
CAMERA_CHECK_INTERVAL=30000
MAX_RECONNECT_ATTEMPTS=10
RECONNECT_DELAY=5000

# Timeout para verificação de câmeras
CAMERA_TIMEOUT=15000

# Ambiente
NODE_ENV=development